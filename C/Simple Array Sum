//Given an array of integers, find the sum of its elements.


#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int i;

    int arsize;
    scanf("%d",&arsize);
int ar[arsize];
    for(i=0;i<arsize;i++){
        scanf("%d",&ar[i]);
        if(ar[i] > 1000){
            printf("error"); break;
        }
   }

  int sum = 0;

    for(i=0;i<arsize;i++){
        sum+=ar[i];
    }
printf("%d",sum);

    return 0;
}
